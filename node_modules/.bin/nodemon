#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nodemon/1.18.6/node_modules/nodemon/bin/nodemon.js/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nodemon/1.18.6/node_modules/nodemon/bin/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nodemon/1.18.6/node_modules/nodemon/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nodemon/1.18.6/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nodemon/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/node_modules:/Users/matt/Documents/PROJECTS/node_modules:/Users/matt/Documents/node_modules:/Users/matt/node_modules:/Users/node_modules:/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/nodemon/bin/nodemon.js/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/nodemon/bin/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/nodemon/node_modules" "$basedir/node"  "$basedir/../nodemon/bin/nodemon.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nodemon/1.18.6/node_modules/nodemon/bin/nodemon.js/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nodemon/1.18.6/node_modules/nodemon/bin/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nodemon/1.18.6/node_modules/nodemon/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nodemon/1.18.6/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nodemon/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/node_modules:/Users/matt/Documents/PROJECTS/node_modules:/Users/matt/Documents/node_modules:/Users/matt/node_modules:/Users/node_modules:/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/nodemon/bin/nodemon.js/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/nodemon/bin/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/nodemon/node_modules" node  "$basedir/../nodemon/bin/nodemon.js" "$@"
  ret=$?
fi
exit $ret
