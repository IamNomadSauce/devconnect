#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nopt/1.0.10/node_modules/nopt/bin/nopt.js/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nopt/1.0.10/node_modules/nopt/bin/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nopt/1.0.10/node_modules/nopt/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nopt/1.0.10/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nopt/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/node_modules:/Users/matt/Documents/PROJECTS/node_modules:/Users/matt/Documents/node_modules:/Users/matt/node_modules:/Users/node_modules:/node_modules" "$basedir/node"  "$basedir/../../../../../../nopt/1.0.10/node_modules/nopt/bin/nopt.js" "$@"
  ret=$?
else 
  NODE_PATH="/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nopt/1.0.10/node_modules/nopt/bin/nopt.js/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nopt/1.0.10/node_modules/nopt/bin/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nopt/1.0.10/node_modules/nopt/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nopt/1.0.10/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/nopt/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules/.registry.npmjs.org/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/devconnect/node_modules:/Users/matt/Documents/PROJECTS/DevConnect/node_modules:/Users/matt/Documents/PROJECTS/node_modules:/Users/matt/Documents/node_modules:/Users/matt/node_modules:/Users/node_modules:/node_modules" node  "$basedir/../../../../../../nopt/1.0.10/node_modules/nopt/bin/nopt.js" "$@"
  ret=$?
fi
exit $ret
